euromeme-ui: sh -c "cd ./apps/euromeme-ui/ && IP=$(ifconfig | grep -Eo 'inet (addr:)?([0-9]*\.){3}[0-9]*' | grep -Eo '([0-9]*\.){3}[0-9]*' | grep -v '127.0.0.1') npm start"
tv-bridge: sh -c 'cd ./apps/tv-bridge/ && npm start'
tv-ui: sh -c "cd ./apps/tv-ui/ && IP=$(ifconfig | grep -Eo 'inet (addr:)?([0-9]*\.){3}[0-9]*' | grep -Eo '([0-9]*\.){3}[0-9]*' | grep -v '127.0.0.1') npm start"
